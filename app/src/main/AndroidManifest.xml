<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prueba_Sonia"
        tools:targetApi="31">

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Prueba_Sonia">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AppHorario.MainActivityHorario"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Prueba_Sonia" />

        <activity
            android:name=".AppHorario.AddClassActivity"
            android:exported="true"
            android:label="@string/add_class"
            android:theme="@style/Theme.Prueba_Sonia" />

        <activity
            android:name=".AppHorario.ViewScheduleActivity"
            android:exported="true"
            android:label="@string/view_schedule"
            android:theme="@style/Theme.Prueba_Sonia" />

        <activity
            android:name=".AppHorario.WhatNowActivity"
            android:exported="true"
            android:label="@string/what_now"
            android:theme="@style/Theme.Prueba_Sonia" />

        <activity
            android:name=".AppEventos.MainActivityEventos"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Prueba_Sonia" />

        <activity
            android:name=".AppEventos.RegisterEventActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Prueba_Sonia" />

    </application>

</manifest>